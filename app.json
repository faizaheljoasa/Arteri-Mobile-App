{
  "expo": {
    "name": "Arteri App",
    "slug": "arteri-app",
    "description": "Arteri Apps PKM UGM 2024",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconArteri.png",
    "scheme": "arteri",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splashArteri.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.pkm2024.arteri",
      "bundleIdentifier": "com.pkm2024.arteri"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pkm2024.arteri"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/icons/notification.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "New notifications"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eff80b07-5b52-492c-9657-5e4a794a9b25"
      }
    },
    "owner": "arteriapps",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eff80b07-5b52-492c-9657-5e4a794a9b25"
    }
  }
}
